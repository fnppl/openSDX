        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="mono"/>
            <xsd:enumeration value="stereo"/>
            <xsd:enumeration value="joint-stereo"/>
            <xsd:enumeration value="5.1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="notemptystring">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="year">
        <xsd:restriction base="xsd:integer">
            <xsd:totalDigits value="4"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="grid">
        <!-- examples: http://en.wikipedia.org/wiki/Global_Release_Identifier
            A12425GABC1234002M
            A1-2425G-ABC1234002-M
            GRid:A1-2425G-ABC1234002-M  -->
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="18"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="amg">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="iswc">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="amzn">
        <!-- example: http://de.wikipedia.org/wiki/Amazon_Standard_Identification_Number -->
        <xsd:restriction base="xsd:integer">

            <xsd:minExclusive value="0"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="contentauth">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="gvl">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="8"/>
            <xsd:pattern value="LC \d{5}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="md5">
        <xsd:restriction base="notemptystring">
            <xsd:minLength value="8"/>
            <xsd:pattern value="([A-F0-9]{2}:){15}[A-F0-9]{2}"/>
            <xsd:pattern value="[a-f0-9]{32}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="sha1">
        <!-- example: E8:27:4E:86:68:9E:CC:67:F0:93:BC:AC:A6:E2:09:C1:C6:25:7D:7B 44
            b2f5171e85760127d04390a7f549da3b669755 -->
        <xsd:restriction base="notemptystring">
            <xsd:minLength value="8"/>
            <xsd:pattern value="([A-F0-9]{2}:){19}[A-F0-9]{2}"/>
            <xsd:pattern value="[a-f0-9]{40}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="sha256">
        <!-- example: 7E:DB:34:A2:E8:38:1C:FE:58:67:97:D0:4F:1A:37:0D:6C:CD:0D:87:62:00:75:FF:FA:71:47:80:DA:A4:8F:38 -->
        <xsd:restriction base="notemptystring">
            <xsd:minLength value="8"/>
            <xsd:pattern value="([A-F0-9]{2}:){31}[A-F0-9]{2}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="notemptystringnospaces">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:pattern value="[^\s]*"/>
            <xsd:whiteSpace value="preserve"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="localization">
    	<xsd:sequence>
	    	<xsd:choice minOccurs="0" maxOccurs="unbounded">
		    	<!-- Define allowed elements within the localization here -->
		    	<xsd:element name="displayname" type="localized" />
		    	<xsd:element name="display_artistname" type="localized" />
		    	<xsd:element name="name" type="localized" />
	    	</xsd:choice>
    	</xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="localized">
    	<xsd:simpleContent>
    		<xsd:extension base="xsd:string">
    			<xsd:attribute name="lang" type="l:language" />
    		</xsd:extension>
    	</xsd:simpleContent>
    </xsd:complexType>
</xsd:schema>
