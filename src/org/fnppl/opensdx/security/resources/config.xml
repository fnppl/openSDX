			<dataname>kaldad</dataname><!-- optional dataname -->
		</data>
		<signoff>
			<keyid>kjakdjadkjajd@keys.fnppl.org</keyid>
			<pubkey>
				<algo>RSA</algo><!-- RSA ; others not implemented yet -->
